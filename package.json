{
  "name": "nvision-data-collection",
  "version": "1.0.0",
  "description": "Nvision Data Collection App",
  "main": "main.js",
  "scripts": {
  "dev": "concurrently \"vite\" \"npm:dev-electron\"",
  "dev-electron": "wait-on http://localhost:5173 && cross-env NODE_ENV=development electron .",
  "build": "vite build && electron-builder",
  "start": "cross-env NODE_ENV=production electron ."
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.787.0",
    "@tailwindcss/vite": "^4.1.2",
    "electron-squirrel-startup": "^1.0.0",
    "jimp": "^0.16.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^29.0.1",
    "electron-builder": "^26.0.12",
    "electronmon": "^2.0.3",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "vite": "^5.1.4",
    "vite-plugin-node-polyfills": "^0.23.0",
    "wait-on": "^8.0.3"
  },
  "build": {
    "appId": "com.nvision.datacollection.testing",
    "productName": "Nvision Data Collection Testing",
    "directories": {
      "buildResources": "assets",
      "output": "dist_electron"
    },
    "files": [
      "dist/**/*",
      "main.js",
      "preload.js",
      "package.json"
    ],
    "win": {
      "target": "nsis"
    }
  }
}
